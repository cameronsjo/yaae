    "esbuild": "^0.27.2",
    "obsidian": "^1.7.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@doc-forge/schemas": "workspace:*",
    "compromise": "^14.14.5",
    "gray-matter": "^4.0.3",
    "zod": "^3.24.0"
  }
