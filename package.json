{
  "name": "yaae",
  "version": "0.1.0",
  "description": "Why Author Anywhere Else â€” an Obsidian plugin",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "pnpm --filter @doc-forge/schemas build && node esbuild.config.mjs production",
    "build:schemas": "pnpm --filter @doc-forge/schemas build",
    "test": "vitest run",
    "test:watch": "vitest",
